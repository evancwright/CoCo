1020 A680             (      FLOODIT.ASM):00351         @lp     LDA ,X+         ;       GET CHAR FROM SCREEN_MEM
1022 B110C2           (      FLOODIT.ASM):00352                 CMPA TEMP       ;       DOES IT MATCH THE 1ST CHAR
1025 260C             (      FLOODIT.ASM):00353                 BNE @no         ;       FOUND A CHAR THAT DIDN'T MATCH THE FIRST
1027 A6A0             (      FLOODIT.ASM):00354                 LDA ,Y+         ;       INC LP COUNTER
1029 108C01DF         (      FLOODIT.ASM):00355                 CMPY #479       ;       HIT END?
102D 26F1             (      FLOODIT.ASM):00356                 BNE @lp         ;       LOOP
102F 8601             (      FLOODIT.ASM):00357                 LDA #1          ;       IF GOT HERE, LOOP DONE, ALL CHARS ON SCRN ARE THE SAME          
1031 2002             (      FLOODIT.ASM):00358                 BRA @r  ;       DONE
1033 8600             (      FLOODIT.ASM):00359         @no     LDA #0  ; 0 = NOT FOUND
1035 3602             (      FLOODIT.ASM):00360         @r      PSHU A
1037 3536             (      FLOODIT.ASM):00361                 PULS Y,X,D
1039 39               (      FLOODIT.ASM):00362                 RTS

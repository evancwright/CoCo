                      (      FLOODIT.ASM):00359         ; SWITCH_BUFFERS
                      (      FLOODIT.ASM):00360         ; THIS ROUTINE COPIES THE SCREEN MEMORY INTO
                      (      FLOODIT.ASM):00361         ; GRAPHICS BUFFER 1. THIS IS REQUIRED PRIOR
                      (      FLOODIT.ASM):00362         ; TO DISPLAYING THE HELP SCREEN SO THE BOARD
                      (      FLOODIT.ASM):00363         ; STATE DOESN'T GET CLOBBERED.
                      (      FLOODIT.ASM):00364         ;
                      (      FLOODIT.ASM):00365         ; Y CONTAINS ADDRESS OF OFFSCREEN BUFFER
                      (      FLOODIT.ASM):00366         ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
1030                  (      FLOODIT.ASM):00367         SWITCH_BUFFERS:
1030 3412             (      FLOODIT.ASM):00368                 PSHS A,X
1032 8E0400           (      FLOODIT.ASM):00369                 LDX #VRAM
1035 A684             (      FLOODIT.ASM):00370         @a      LDA ,X          ;SAVE SCREEN BYTE
1037 B7107E           (      FLOODIT.ASM):00371                 STA TEMP
103A A6A4             (      FLOODIT.ASM):00372                 LDA ,Y          ;LOAD BUFFER BYTE
103C A784             (      FLOODIT.ASM):00373                 STA ,X          ;COPY BUFFER BYTE TO SCREEN
103E B6107E           (      FLOODIT.ASM):00374                 LDA TEMP        ;GET SCREEN BYTE BACK
1041 A7A4             (      FLOODIT.ASM):00375                 STA ,Y          ;COPY SCREEN TO BUF
1043 A680             (      FLOODIT.ASM):00376                 LDA ,X+ ; INC X
1045 A6A0             (      FLOODIT.ASM):00377                 LDA ,Y+ ; INC Y
1047 8C0600           (      FLOODIT.ASM):00378                 CMPX #VRAM_END ; DONE?
104A 26E9             (      FLOODIT.ASM):00379                 BNE @a
104C 3512             (      FLOODIT.ASM):00380                 PULS X,A
104E 39               (      FLOODIT.ASM):00381                 RTS

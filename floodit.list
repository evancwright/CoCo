                      (      FLOODIT.ASM):00322         ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
0FF3                  (      FLOODIT.ASM):00323         CLEAR_FLAGS:
0FF3 108E0000         (      FLOODIT.ASM):00324                 LDY #0          ;loop counter
0FF7 8E10A4           (      FLOODIT.ASM):00325                 LDX #FLAGS      ;start addr
0FFA 9600             (      FLOODIT.ASM):00326         @a      LDA 0
0FFC A780             (      FLOODIT.ASM):00327                 STA ,X+         ;clear the byte
0FFE A6A0             (      FLOODIT.ASM):00328                 LDA ,Y+         ;inc loop counter (clobbers a)
1000 108C01E0         (      FLOODIT.ASM):00329                 CMPY #480
1004 26F4             (      FLOODIT.ASM):00330                 BNE @a
1006 39               (      FLOODIT.ASM):00331                 RTS
                      (      FLOODIT.ASM):00332         
                      (      FLOODIT.ASM):00333         ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
                      (      FLOODIT.ASM):00334         ;CHECK WIN
                      (      FLOODIT.ASM):00335         ;SCAN THE SCREEN MEMORY LOOKING FOR A CHAR THAT
                      (      FLOODIT.ASM):00336         ;DOESN'T MATCH THE FIRST.  THE RESULT (0 or 1)
                      (      FLOODIT.ASM):00337         ;IS RETURNED ON THE USER STACK.
                      (      FLOODIT.ASM):00338         ;REGISTERS ARE NOT AFFECTED
                      (      FLOODIT.ASM):00339         ;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;;
1007                  (      FLOODIT.ASM):00340         CHECK_WIN:
1007 3436             (      FLOODIT.ASM):00341                 PSHS D,X,Y
1009 108E0000         (      FLOODIT.ASM):00342                 LDY #0          ; LOOP COUNTER
100D 8E0401           (      FLOODIT.ASM):00343                 LDX #VRAM+1     ;       ADDRESS OF VIDEO RAM
1010 B60400           (      FLOODIT.ASM):00344                 LDA VRAM        ;       LOAD 1ST CHAR OF VRAM
1013 B7107E           (      FLOODIT.ASM):00345                 STA TEMP        ;       SAVE 1ST CHAR IN TEMP   
1016 A680             (      FLOODIT.ASM):00346         @lp     LDA ,X+         ;       GET CHAR FROM SCREEN_MEM
1018 B1107E           (      FLOODIT.ASM):00347                 CMPA TEMP       ;       DOES IT MATCH THE 1ST CHAR
101B 260C             (      FLOODIT.ASM):00348                 BNE @no         ;       FOUND A CHAR THAT DIDN'T MATCH THE FIRST
101D A6A0             (      FLOODIT.ASM):00349                 LDA ,Y+         ;       INC LP COUNTER
101F 108C01DF         (      FLOODIT.ASM):00350                 CMPY #479       ;       HIT END?
1023 26F1             (      FLOODIT.ASM):00351                 BNE @lp         ;       LOOP
1025 8601             (      FLOODIT.ASM):00352                 LDA #1          ;       IF GOT HERE, LOOP DONE, ALL CHARS ON SCRN ARE THE SAME          
1027 2002             (      FLOODIT.ASM):00353                 BRA @r  ;       DONE
1029 8600             (      FLOODIT.ASM):00354         @no     LDA #0  ; 0 = NOT FOUND
102B 3602             (      FLOODIT.ASM):00355         @r      PSHU A
102D 3536             (      FLOODIT.ASM):00356                 PULS Y,X,D
102F 39               (      FLOODIT.ASM):00357                 RTS
